{
    "version": 2,
    "buildCommand": "npm install -g node-gyp && npm run vercel-build",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/css/*",
            "dest": "public/css/$name"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}